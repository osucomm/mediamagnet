<address>
  <%- if contact.name.present? %>
    <strong><%= contact.name %></strong><br>
  <%- end %>
  <%- if contact.organization.present? %>
    <%= contact.organization %></br>
  <%- end %>
  <%- if contact.phone.present? %>
    <%= contact.phone %> |
  <%- end %>
  <%- if contact.email.present? %>
    <%= mail_to contact.email %> |
  <%- end %>
  <%- if contact.url.present? %>
   <%= link_to contact.url, contact.url %>
  <%- end %>
</address>
