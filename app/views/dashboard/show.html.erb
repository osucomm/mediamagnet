<h1><%= title 'Dashboard' %></h1>
<div class="col-sm-4">
  <h2>Entities</h2>
  <%= link_to (fa_icon "plus", text: "Create"), new_entity_path %>
  <h3>Your Entities</h3>
  <ul>
    <%- current_user.entities.each do |entity| %>
      <li><%= link_to entity.name, entity %></li>
    <%- end %>
  </ul>
</div>
<div class="col-sm-4">
  <h2>Items</h2>
</div>
<div class="col-sm-4">
  <h2>Something Else?</h2>
</div>
